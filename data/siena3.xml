<?xml version="1.0" encoding="UTF-8"?><ca:CompositeApplicationType xmlns:ca="http://siena.ibm.com/model/CompositeApplication" version="2.0" name="application">
  <ca:EventModel id="Definitions_1_eventModel" name="Definitions_1_eventModel">
    <ca:Event id="Container_e" name="Container_e"/>
    <ca:Event id="Container_l" name="Container_l"/>
    <ca:Event id="Truck_e" name="Truck_e"/>
    <ca:Event id="Truck_l" name="Truck_l"/>
    <ca:Event id="DayOfWeek_e" name="DayOfWeek_e"/>
    <ca:Event id="DayOfWeek_l" name="DayOfWeek_l"/>
    <ca:Event id="Delivery_notification" name="Delivery_notification"/>
    <ca:Event id="Start_shipping" name="Start_shipping"/>
  </ca:EventModel>
  <ca:Component id="model" name="model">
    <ca:InformationModel id="infoModel" rootDataItemId="infoModel">
      <ca:DataItem id="infoModel" rootElement="infoModel" schemaUri="data/infoModel.xsd"/>
    </ca:InformationModel>
    <ca:GuardedStageModel>
<ca:Stage id="shippingProcess" name="Default Process">
        <ca:DataFlowGuard eventIds="Start_shipping" expression="GSM.isEventOccurring(Start_shipping)" id="shippingProcess_dfg1" language="JEXL" name="shippingProcess Data Flow Guard 1"/>
        <ca:DataFlowGuard eventIds="Truck_e" expression="(({infoModel./infoModel/Truck/status} == [Producer])) and (({infoModel./infoModel/Container/status} == [Full])) and GSM.isEventOccurring(Truck_e)" id="shippingProcess_dfg2" language="JEXL" name="shippingProcess Data Flow Guard 2"/>
        <ca:DataFlowGuard eventIds="Container_e" expression="(({infoModel./infoModel/Truck/status} == [Producer])) and (({infoModel./infoModel/Container/status} == [Full])) and GSM.isEventOccurring(Container_e)" id="shippingProcess_dfg3" language="JEXL" name="shippingProcess Data Flow Guard 3"/>
        <ca:DataFlowGuard eventIds="Truck_e" expression="(({infoModel./infoModel/Truck/status} == [HighwayToB])) and GSM.isEventOccurring(Truck_e)" id="shippingProcess_dfg4" language="JEXL" name="shippingProcess Data Flow Guard 4"/>
        <ca:DataFlowGuard eventIds="Truck_e" expression="(({infoModel./infoModel/Truck/status} == [HighwayToC])) and GSM.isEventOccurring(Truck_e)" id="shippingProcess_dfg5" language="JEXL" name="shippingProcess Data Flow Guard 5"/>
        <ca:DataFlowGuard eventIds="Truck_e" expression="(({infoModel./infoModel/Truck/status} == [SiteB]) or ({infoModel./infoModel/Truck/status} == [SiteC])) and GSM.isEventOccurring(Truck_e)" id="shippingProcess_dfg6" language="JEXL" name="shippingProcess Data Flow Guard 6"/>
        <ca:DataFlowGuard eventIds="Delivery_notification" expression="GSM.isEventOccurring(Delivery_notification)" id="shippingProcess_dfg7" language="JEXL" name="shippingProcess Data Flow Guard 7"/>
        <ca:Milestone eventIds="" id="shippingProcess_m1" name="shippingProcess Milestone 1">
          <ca:Condition expression="GSM.isMilestoneAchieved(Delivery_notification_m1) and (GSM.isMilestoneAchieved(DeliverToCustomer_m1)) and GSM.isMilestoneAchieved(Start_shipping_m1) and (GSM.isMilestoneAchieved(ShipToA_m1)) and GSM.isMilestoneAchieved(ExclusiveGateway_1_m1)" id="shippingProcess_m1_c" language="JEXL" name="shippingProcess Milestone 1 Condition"/>
        </ca:Milestone>
        <ca:SubStage id="Delivery_notification" name="Delivery_notification">
          <ca:DataFlowGuard eventIds="Delivery_notification" expression="GSM.isEventOccurring(Delivery_notification)" id="Delivery_notification_dfg1" language="JEXL" name="Delivery_notification Data Flow Guard 1"/>
          <ca:Milestone eventIds="Delivery_notification" id="Delivery_notification_m1" name="Delivery_notification Milestone 1">
            <ca:Condition expression="GSM.isEventOccurring(Delivery_notification)" id="Delivery_notification_m1_c" language="JEXL" name="Delivery_notification Milestone 1 Condition"/>
          </ca:Milestone>
          <ca:ProcessFlowGuard id="Delivery_notification_pfg" name="Delivery_notification Process Flow Guard" expression="not GSM.isMilestoneAchieved(Delivery_notification_m1)  and (GSM.isMilestoneAchieved(DeliverToCustomer_m1))"/>
        </ca:SubStage>
        <ca:SubStage id="DeliverToCustomer" name="DeliverToCustomer">
          <ca:DataFlowGuard eventIds="Truck_e" expression="(({infoModel./infoModel/Truck/status} == [SiteB]) or ({infoModel./infoModel/Truck/status} == [SiteC])) and GSM.isEventOccurring(Truck_e)" id="DeliverToCustomer_dfg1" language="JEXL" name="DeliverToCustomer Data Flow Guard 1"/>
          <ca:Milestone eventIds="Truck_l" id="DeliverToCustomer_m1" name="DeliverToCustomer Milestone 1">
            <ca:Condition expression="(({infoModel./infoModel/Truck/status} == [Customer])) and GSM.isEventOccurring(Truck_l)" id="DeliverToCustomer_m1_c" language="JEXL" name="DeliverToCustomer Milestone 1 Condition"/>
          </ca:Milestone>
          <ca:ProcessFlowGuard id="DeliverToCustomer_pfg" name="DeliverToCustomer Process Flow Guard" expression="not (GSM.isMilestoneAchieved(DeliverToCustomer_m1)) and GSM.isMilestoneAchieved(ExclusiveGateway_1_m1)"/>
        </ca:SubStage>
        <ca:SubStage id="Start_shipping" name="Start_shipping">
          <ca:DataFlowGuard eventIds="Start_shipping" expression="GSM.isEventOccurring(Start_shipping)" id="Start_shipping_dfg1" language="JEXL" name="Start_shipping Data Flow Guard 1"/>
          <ca:Milestone eventIds="Start_shipping" id="Start_shipping_m1" name="Start_shipping Milestone 1">
            <ca:Condition expression="GSM.isEventOccurring(Start_shipping)" id="Start_shipping_m1_c" language="JEXL" name="Start_shipping Milestone 1 Condition"/>
          </ca:Milestone>
          <ca:ProcessFlowGuard id="Start_shipping_pfg" name="Start_shipping Process Flow Guard" expression="not GSM.isMilestoneAchieved(Start_shipping_m1) "/>
        </ca:SubStage>
        <ca:SubStage id="ShipToA" name="ShipToA">
          <ca:DataFlowGuard eventIds="Truck_e" expression="(({infoModel./infoModel/Truck/status} == [Producer])) and (({infoModel./infoModel/Container/status} == [Full])) and GSM.isEventOccurring(Truck_e)" id="ShipToA_dfg1" language="JEXL" name="ShipToA Data Flow Guard 1"/>
          <ca:DataFlowGuard eventIds="Container_e" expression="(({infoModel./infoModel/Truck/status} == [Producer])) and (({infoModel./infoModel/Container/status} == [Full])) and GSM.isEventOccurring(Container_e)" id="ShipToA_dfg2" language="JEXL" name="ShipToA Data Flow Guard 2"/>
          <ca:Milestone eventIds="Truck_l" id="ShipToA_m1" name="ShipToA Milestone 1">
            <ca:Condition expression="(({infoModel./infoModel/Truck/status} == [SiteA])) and GSM.isEventOccurring(Truck_l)" id="ShipToA_m1_c" language="JEXL" name="ShipToA Milestone 1 Condition"/>
          </ca:Milestone>
          <ca:ProcessFlowGuard id="ShipToA_pfg" name="ShipToA Process Flow Guard" expression="not (GSM.isMilestoneAchieved(ShipToA_m1)) and GSM.isMilestoneAchieved(Start_shipping_m1)"/>
        </ca:SubStage>
        <ca:SubStage id="ExclusiveGateway_1" name="ExclusiveGateway_1">
          <ca:DataFlowGuard eventIds="Truck_e" expression="(({infoModel./infoModel/Truck/status} == [HighwayToB])) and GSM.isEventOccurring(Truck_e)" id="ExclusiveGateway_1_dfg1" language="JEXL" name="ExclusiveGateway_1 Data Flow Guard 1"/>
          <ca:DataFlowGuard eventIds="Truck_e" expression="(({infoModel./infoModel/Truck/status} == [HighwayToC])) and GSM.isEventOccurring(Truck_e)" id="ExclusiveGateway_1_dfg2" language="JEXL" name="ExclusiveGateway_1 Data Flow Guard 2"/>
          <ca:Milestone eventIds="" id="ExclusiveGateway_1_m1" name="ExclusiveGateway_1 Milestone 1">
            <ca:Condition expression="(GSM.isMilestoneAchieved(SequenceFlow_6_m1) and ({infoModel./infoModel/DayOfWeek/status} == [Sunday])) or (GSM.isMilestoneAchieved(SequenceFlow_11_m1) and (not ({infoModel./infoModel/DayOfWeek/status} == [Sunday]))) and not (GSM.isStageActive(SequenceFlow_6) or GSM.isStageActive(SequenceFlow_11))" id="ExclusiveGateway_1_m1_c" language="JEXL" name="ExclusiveGateway_1 Milestone 1 Condition"/>
          </ca:Milestone>
          <ca:SubStage id="SequenceFlow_11">
            <ca:DataFlowGuard eventIds="Truck_e" expression="(({infoModel./infoModel/Truck/status} == [HighwayToC])) and GSM.isEventOccurring(Truck_e)" id="SequenceFlow_11_dfg1" language="JEXL" name="SequenceFlow_11 Data Flow Guard 1"/>
            <ca:Milestone eventIds="" id="SequenceFlow_11_m1" name="SequenceFlow_11 Milestone 1">
              <ca:Condition expression="(GSM.isMilestoneAchieved(ShipToC_m1))" id="SequenceFlow_11_m1_c" language="JEXL" name="SequenceFlow_11 Milestone 1 Condition"/>
            </ca:Milestone>
            <ca:SubStage id="ShipToC" name="ShipToC">
              <ca:DataFlowGuard eventIds="Truck_e" expression="(({infoModel./infoModel/Truck/status} == [HighwayToC])) and GSM.isEventOccurring(Truck_e)" id="ShipToC_dfg1" language="JEXL" name="ShipToC Data Flow Guard 1"/>
              <ca:Milestone eventIds="Truck_l" id="ShipToC_m1" name="ShipToC Milestone 1">
                <ca:Condition expression="(({infoModel./infoModel/Truck/status} == [SiteC])) and GSM.isEventOccurring(Truck_l)" id="ShipToC_m1_c" language="JEXL" name="ShipToC Milestone 1 Condition"/>
              </ca:Milestone>
              <ca:ProcessFlowGuard id="ShipToC_pfg" name="ShipToC Process Flow Guard" expression="not (GSM.isMilestoneAchieved(ShipToC_m1))"/>
            </ca:SubStage>
            <ca:ProcessFlowGuard id="SequenceFlow_11_pfg" expression="(not (GSM.isMilestoneAchieved(SequenceFlow_11_m1))) and (not (({infoModel./infoModel/DayOfWeek/status} == [Sunday]))) and (not (GSM.isStageActive(SequenceFlow_6)))"/>
          </ca:SubStage>
          <ca:SubStage id="SequenceFlow_6">
            <ca:DataFlowGuard eventIds="Truck_e" expression="(({infoModel./infoModel/Truck/status} == [HighwayToB])) and GSM.isEventOccurring(Truck_e)" id="SequenceFlow_6_dfg1" language="JEXL" name="SequenceFlow_6 Data Flow Guard 1"/>
            <ca:Milestone eventIds="" id="SequenceFlow_6_m1" name="SequenceFlow_6 Milestone 1">
              <ca:Condition expression="(GSM.isMilestoneAchieved(ShipToB_m1))" id="SequenceFlow_6_m1_c" language="JEXL" name="SequenceFlow_6 Milestone 1 Condition"/>
            </ca:Milestone>
            <ca:SubStage id="ShipToB" name="ShipToB">
              <ca:DataFlowGuard eventIds="Truck_e" expression="(({infoModel./infoModel/Truck/status} == [HighwayToB])) and GSM.isEventOccurring(Truck_e)" id="ShipToB_dfg1" language="JEXL" name="ShipToB Data Flow Guard 1"/>
              <ca:Milestone eventIds="Truck_l" id="ShipToB_m1" name="ShipToB Milestone 1">
                <ca:Condition expression="(({infoModel./infoModel/Truck/status} == [SiteB])) and GSM.isEventOccurring(Truck_l)" id="ShipToB_m1_c" language="JEXL" name="ShipToB Milestone 1 Condition"/>
              </ca:Milestone>
              <ca:ProcessFlowGuard id="ShipToB_pfg" name="ShipToB Process Flow Guard" expression="not (GSM.isMilestoneAchieved(ShipToB_m1))"/>
            </ca:SubStage>
            <ca:ProcessFlowGuard id="SequenceFlow_6_pfg" expression="(not (GSM.isMilestoneAchieved(SequenceFlow_6_m1))) and (({infoModel./infoModel/DayOfWeek/status} == [Sunday])) and (not (GSM.isStageActive(SequenceFlow_11)))"/>
          </ca:SubStage>
          <ca:ProcessFlowGuard id="ExclusiveGateway_1_pfg" name="ExclusiveGateway_1 Process Flow Guard" expression="not GSM.isMilestoneAchieved(ExclusiveGateway_1_m1) and (GSM.isMilestoneAchieved(ShipToA_m1))"/>
        </ca:SubStage>
      </ca:Stage>
      <ca:Stage id="Truck_LC" name="Truck LifeCycle">
        <ca:DataFlowGuard eventIds="Truck_l" expression="true" id="Truck_dfg1" language="JEXL" name="Truck Data Flow Guard"/>
        <ca:Milestone id="Truck_m1" name="Truck Milestone">
          <ca:Condition expression="false" id="Truck_m1_c" language="JEXL" name="Truck Milestone Condition"/>
        </ca:Milestone>
        <ca:SubStage id="Truck_Producer" name="Producer">
          <ca:DataFlowGuard eventIds="Truck_l" expression="({infoModel./infoModel/Truck/status} == [Producer]) and GSM.isEventOccurring(Truck_l)" id="Truck_Producer_dfg1" language="JEXL" name="Producer Data Flow Guard"/>
          <ca:Milestone eventIds="Truck_e" id="Truck_Producer_m1" name="Producer Milestone">
            <ca:Condition expression="not ({infoModel./infoModel/Truck/status} == [Producer]) and GSM.isEventOccurring(Truck_e)" id="Truck_Producer_m1_c" language="JEXL" name="Producer Milestone Condition"/>
          </ca:Milestone>
          <ca:ProcessFlowGuard id="Truck_Producer_pfg1" name="Producer Process Flow Guard" expression="not (GSM.isStageActive(Truck_SiteA) or GSM.isStageActive(Truck_HighwayToB) or GSM.isStageActive(Truck_SiteB) or GSM.isStageActive(Truck_Customer) or GSM.isStageActive(Truck_HighwayToC) or GSM.isStageActive(Truck_SiteC) or GSM.isStageActive(Truck_error))"/>
        </ca:SubStage>
        <ca:SubStage id="Truck_SiteA" name="SiteA">
          <ca:DataFlowGuard eventIds="Truck_l" expression="({infoModel./infoModel/Truck/status} == [SiteA]) and GSM.isEventOccurring(Truck_l)" id="Truck_SiteA_dfg1" language="JEXL" name="SiteA Data Flow Guard"/>
          <ca:Milestone eventIds="Truck_e" id="Truck_SiteA_m1" name="SiteA Milestone">
            <ca:Condition expression="not ({infoModel./infoModel/Truck/status} == [SiteA]) and GSM.isEventOccurring(Truck_e)" id="Truck_SiteA_m1_c" language="JEXL" name="SiteA Milestone Condition"/>
          </ca:Milestone>
          <ca:ProcessFlowGuard id="Truck_SiteA_pfg1" name="SiteA Process Flow Guard" expression="GSM.isStageActive(Truck_Producer)"/>
        </ca:SubStage>
        <ca:SubStage id="Truck_HighwayToB" name="HighwayToB">
          <ca:DataFlowGuard eventIds="Truck_l" expression="({infoModel./infoModel/Truck/status} == [HighwayToB]) and GSM.isEventOccurring(Truck_l)" id="Truck_HighwayToB_dfg1" language="JEXL" name="HighwayToB Data Flow Guard"/>
          <ca:Milestone eventIds="Truck_e" id="Truck_HighwayToB_m1" name="HighwayToB Milestone">
            <ca:Condition expression="not ({infoModel./infoModel/Truck/status} == [HighwayToB]) and GSM.isEventOccurring(Truck_e)" id="Truck_HighwayToB_m1_c" language="JEXL" name="HighwayToB Milestone Condition"/>
          </ca:Milestone>
          <ca:ProcessFlowGuard id="Truck_HighwayToB_pfg1" name="HighwayToB Process Flow Guard" expression="GSM.isStageActive(Truck_SiteA)"/>
        </ca:SubStage>
        <ca:SubStage id="Truck_SiteB" name="SiteB">
          <ca:DataFlowGuard eventIds="Truck_l" expression="({infoModel./infoModel/Truck/status} == [SiteB]) and GSM.isEventOccurring(Truck_l)" id="Truck_SiteB_dfg1" language="JEXL" name="SiteB Data Flow Guard"/>
          <ca:Milestone eventIds="Truck_e" id="Truck_SiteB_m1" name="SiteB Milestone">
            <ca:Condition expression="not ({infoModel./infoModel/Truck/status} == [SiteB]) and GSM.isEventOccurring(Truck_e)" id="Truck_SiteB_m1_c" language="JEXL" name="SiteB Milestone Condition"/>
          </ca:Milestone>
          <ca:ProcessFlowGuard id="Truck_SiteB_pfg1" name="SiteB Process Flow Guard" expression="GSM.isStageActive(Truck_HighwayToB)"/>
        </ca:SubStage>
        <ca:SubStage id="Truck_Customer" name="Customer">
          <ca:DataFlowGuard eventIds="Truck_l" expression="({infoModel./infoModel/Truck/status} == [Customer]) and GSM.isEventOccurring(Truck_l)" id="Truck_Customer_dfg1" language="JEXL" name="Customer Data Flow Guard"/>
          <ca:Milestone eventIds="Truck_e" id="Truck_Customer_m1" name="Customer Milestone">
            <ca:Condition expression="not ({infoModel./infoModel/Truck/status} == [Customer]) and GSM.isEventOccurring(Truck_e)" id="Truck_Customer_m1_c" language="JEXL" name="Customer Milestone Condition"/>
          </ca:Milestone>
          <ca:ProcessFlowGuard id="Truck_Customer_pfg1" name="Customer Process Flow Guard" expression="GSM.isStageActive(Truck_SiteB) or GSM.isStageActive(Truck_SiteC)"/>
        </ca:SubStage>
        <ca:SubStage id="Truck_HighwayToC" name="HighwayToC">
          <ca:DataFlowGuard eventIds="Truck_l" expression="({infoModel./infoModel/Truck/status} == [HighwayToC]) and GSM.isEventOccurring(Truck_l)" id="Truck_HighwayToC_dfg1" language="JEXL" name="HighwayToC Data Flow Guard"/>
          <ca:Milestone eventIds="Truck_e" id="Truck_HighwayToC_m1" name="HighwayToC Milestone">
            <ca:Condition expression="not ({infoModel./infoModel/Truck/status} == [HighwayToC]) and GSM.isEventOccurring(Truck_e)" id="Truck_HighwayToC_m1_c" language="JEXL" name="HighwayToC Milestone Condition"/>
          </ca:Milestone>
          <ca:ProcessFlowGuard id="Truck_HighwayToC_pfg1" name="HighwayToC Process Flow Guard" expression="GSM.isStageActive(Truck_SiteA)"/>
        </ca:SubStage>
        <ca:SubStage id="Truck_SiteC" name="SiteC">
          <ca:DataFlowGuard eventIds="Truck_l" expression="({infoModel./infoModel/Truck/status} == [SiteC]) and GSM.isEventOccurring(Truck_l)" id="Truck_SiteC_dfg1" language="JEXL" name="SiteC Data Flow Guard"/>
          <ca:Milestone eventIds="Truck_e" id="Truck_SiteC_m1" name="SiteC Milestone">
            <ca:Condition expression="not ({infoModel./infoModel/Truck/status} == [SiteC]) and GSM.isEventOccurring(Truck_e)" id="Truck_SiteC_m1_c" language="JEXL" name="SiteC Milestone Condition"/>
          </ca:Milestone>
          <ca:ProcessFlowGuard id="Truck_SiteC_pfg1" name="SiteC Process Flow Guard" expression="GSM.isStageActive(Truck_HighwayToC)"/>
        </ca:SubStage>
        <ca:SubStage id="Truck_error" name="Truck error">
          <ca:DataFlowGuard expression="not (({infoModel./infoModel/Truck/status} == [Producer]) or ({infoModel./infoModel/Truck/status} == [SiteA]) or ({infoModel./infoModel/Truck/status} == [HighwayToB]) or ({infoModel./infoModel/Truck/status} == [SiteB]) or ({infoModel./infoModel/Truck/status} == [Customer]) or ({infoModel./infoModel/Truck/status} == [HighwayToC]) or ({infoModel./infoModel/Truck/status} == [SiteC])) and GSM.isEventOccurring(Truck_l)" id="Truck_error_dfg1" language="JEXL" name="Truck Error Data Flow Guard"/>
          <ca:Milestone id="Truck_error_m1" name="Truck Error Milestone">
            <ca:Condition expression="(({infoModel./infoModel/Truck/status} == [Producer]) or ({infoModel./infoModel/Truck/status} == [SiteA]) or ({infoModel./infoModel/Truck/status} == [HighwayToB]) or ({infoModel./infoModel/Truck/status} == [SiteB]) or ({infoModel./infoModel/Truck/status} == [Customer]) or ({infoModel./infoModel/Truck/status} == [HighwayToC]) or ({infoModel./infoModel/Truck/status} == [SiteC])) and GSM.isEventOccurring(Truck_e)" id="Truck_error_m1_c" language="JEXL" name="Truck Error Milestone Condition"/>
          </ca:Milestone>
          <ca:ProcessFlowGuard type="application:DataFlowGuardType" expression="false" id="Truck_error_pfg1" language="JEXL" name="Truck Error Process Flow Guard"/>
        </ca:SubStage>
        <ca:SubStage id="Truck_final" name="Truck final">
          <ca:DataFlowGuard expression="(GSM.isStageActive(Truck_Customer))" id="Truck_final_dfg1" language="JEXL" name="Truck Final Data Flow Guard"/>
          <ca:Milestone id="Truck_final_m1" name="Truck Final Milestone">
            <ca:Condition expression="not (GSM.isStageActive(Truck_Customer))" id="Truck_final_m1_c" language="JEXL" name="Truck Final Milestone Condition"/>
          </ca:Milestone>
        </ca:SubStage>
      </ca:Stage>
    </ca:GuardedStageModel>
  </ca:Component>
</ca:CompositeApplicationType>
